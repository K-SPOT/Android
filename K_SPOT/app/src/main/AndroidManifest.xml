<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="k_spot.jnm.k_spot">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MainActivity"
            android:fitsSystemWindows="true" />
        <activity
            android:name=".activity.DetailedEntertainerPageActivity"
            android:fitsSystemWindows="true"
            android:label="@string/title_activity_detailed_entertainer_page"
            android:theme="@style/AppTheme" />
        <activity android:name=".activity.SpotViewMoreActivity" />
        <activity android:name=".activity.RecommendViewMoreActivity" />
        <activity android:name=".SearchActivity"></activity>
        <activity android:name=".SearchResultActivity">

        </activity>
        <activity android:name=".SearchBraodViewMoreActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>